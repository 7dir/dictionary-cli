{
  "name": "dictionary-cli",
  "version": "2.0.1",
  "description": "Translate and look up usage examples via yandex services",
  "scripts": {
    "build": "babel -d dist src",
    "build:watch": "babel --watch -d dist src",
    "prepublish": "npm run build",
    "postpublish": "rm -rf dist",
    "test": "npm run build && nyc --reporter=lcov --reporter=text ava"
  },
  "bin": {
    "dictionary": "cli.js"
  },
  "files": [
    "cli.js",
    "dist"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/dictionary-cli.git"
  },
  "keywords": [
    "yandex",
    "translate",
    "translator",
    "dictionary",
    "cli"
  ],
  "author": "ewnd9 <ewndnine@gmail.com>",
  "dependencies": {
    "chalk": "^1.1.1",
    "dot-file-config": "^0.6.0",
    "got": "^6.3.0",
    "lodash": "^4.13.1",
    "meow": "^3.7.0",
    "update-notifier": "^1.0.2"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-plugin-lodash": "^3.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "coveralls": "^2.11.6",
    "get-stream": "^2.3.0",
    "nyc": "^6.6.1"
  }
}
